;; Add Some Shotcuts In Popup Menu Mode
(define-key popup-menu-key-map (kbd "M-n") 'popup-next)
(define-key popup-menu-key-map (kbd "TAB") 'popup-next)
(define-key popup-menu-key-map (kbd "<tab>") 'popup-next)
(define-key popup-menu-key-map (kbd "<backtab>") 'popup-previous)
(define-key popup-menu-key-map (kbd "M-p") 'popup-previous)
